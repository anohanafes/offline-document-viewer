{
  "name": "@anohanafes/vue-document-viewer",
  "version": "1.0.0",
  "description": "A Vue 2 component for previewing documents (PDF, DOCX, PPTX, XLSX) online without server dependencies",
  "main": "dist/vue-document-viewer.common.js",
  "module": "dist/vue-document-viewer.esm.js",
  "unpkg": "dist/vue-document-viewer.umd.js",
  "files": [
    "dist/*",
    "src/*",
    "public/lib/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue2",
    "document",
    "viewer",
    "preview",
    "pdf",
    "docx", 
    "pptx",
    "xlsx",
    "office",
    "document-preview",
    "file-viewer",
    "online-preview"
  ],
  "author": "anohanafes <519855937@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anohanafes/offline-document-viewer.git",
    "directory": "vue-document-viewer"
  },
  "bugs": {
    "url": "https://github.com/anohanafes/offline-document-viewer/issues"
  },
  "homepage": "https://github.com/anohanafes/offline-document-viewer/tree/main/vue-document-viewer#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vue-document-viewer src/index.js",
    "build:demo": "vue-cli-service build --dest demo-dist",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^2.6.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "jszip": "^3.10.1",
